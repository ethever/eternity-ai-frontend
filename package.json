{
  "name": "eternity-ai",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pnpm run -r build",
    "build:web": "pnpm run --filter ./apps/official_web build",
    "build:webv2": "pnpm run --filter ./apps/web_v2 build",
    "build:main": "pnpm run --filter ./apps/main build",
    "dev": "pnpm -r dev",
    "dev:web": "pnpm --filter ./apps/official_web dev",
    "dev:webv2": "pnpm --filter ./apps/web_v2 dev",
    "dev:main": "pnpm --filter ./apps/main dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish:main": "pnpm wrangler pages publish ./apps/main/dist --project-name=eternity-ai-official-website-frontend",
    "publish:web": "pnpm wrangler pages publish ./apps/web_v2/dist --project-name=eternity-ai-dapp-frontend"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "preinstall": "npx only-allow pnpm",
  "dependencies": {
    "react-hot-toast": "^2.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "wrangler": "^2.19.0"
  }
}
